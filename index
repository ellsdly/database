SELECT * FROM TAK18_Barlin.yl21_Books;
SELECT * 
FROM TAK18_Barlin.yl21_Books 
WHERE release_date>2010 
AND type="new" 
ORDER BY title;

#ülesanne3
SELECT * 
FROM TAK18_Barlin.yl21_Books 
WHERE release_date<1970
AND type="used" 
AND price<20;

#ülesanne4
SELECT COUNT(*), year(order_date)
FROM TAK18_Barlin.yl21_Orders 
GROUP BY YEAR (order_date)
;
